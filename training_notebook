        "from sklearn.preprocessing import StandardScaler\n",
        "\n",
        "\n",
        "y_train_mod = y_train_mod[y_train['CM'] >= 0]\n",
        "\n",
        "# All the values above 100000 are assigned to 100000\n",
        "\n",
        "y_train_mod.loc[y_train_mod['CM'] > 100000, 'CM'] = 100000\n",
        "\n",
        "y_train_mod.reset_index(drop=True, inplace=True)\n",
        "\n",
        "\n",
        "# Reshape `y_train['CM']` to a 2D array (required by scaler)\n",
        "\n",
        "y_train_mod.loc[:,'CM_log'] = np.log1p(y_train_mod['CM'])\n"
      ]
