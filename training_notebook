        "# Get feature importances from the trained model\n",
        "feature_importances = rf_best_sklearn_cm_x_freq_without.feature_importances_\n",
        "\n",
        "# Convert to DataFrame for better visualization\n",
        "feature_importance_df = pd.DataFrame({\n",
        "    'Feature': X_train_mod_2.columns,  # Use columns from the original DataFrame\n",
        "    'Importance': feature_importances\n",
        "})\n",
        "\n",
        "# Sort by importance\n",
        "feature_importance_df = feature_importance_df.sort_values(by=\"Importance\", ascending=False)\n",
        "\n",
        "# Display the top features\n",
        "print(feature_importance_df)\n",
        "\n",
        "list_index_corr = []\n",
        "for index, el in enumerate(feature_importance_df['Importance']) :\n",
        "\n",
        "  if el > 0.01 :\n",
        "\n",
        "    list_index_corr.append(index)\n",
        "    print(f\"Correlation found with {el} : {feature_importance_df['Feature'].loc[index]} \")\n",
        "\n",
        "\n",
        "# Plot feature importance\n",
        "plt.figure(figsize=(10, 6))\n",
        "plt.barh(feature_importance_df['Feature'].loc[list_index_corr], feature_importance_df['Importance'].loc[list_index_corr], color='skyblue')\n",
        "plt.gca().invert_yaxis()  # Invert y-axis to have the most important feature on top\n",
        "plt.xlabel('Importance')\n",
        "plt.ylabel('Feature')\n",
        "plt.title('Model CM x FREQ (without) - Feature Importance - RF (Scikit-Learn)')\n",
        "plt.show()\n"
      ],
