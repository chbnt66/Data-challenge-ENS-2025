        "# Initialize OrdinalEncoder\n",
        "encoder = OrdinalEncoder(handle_unknown=\"use_encoded_value\", unknown_value=-1)  # Handle unseen categories\n",
        "\n",
        "# Fit-transform training data\n",
        "X_train_encoded = encoder.fit_transform(X_train_mod)\n",
        "\n",
        "# Transform test data (use same encoder)\n",
        "X_test_encoded = encoder.transform(X_test)\n",
        "\n",
        "# Convert back to DataFrame (optional)\n",
        "X_train_mod = pd.DataFrame(X_train_encoded, columns=X_train_mod.columns)\n",
        "X_test = pd.DataFrame(X_test_encoded, columns=X_test.columns)\n",
        "\n",
        "print(f\"Training set shape (after Ordinal Encoding): {X_train_mod.shape}\")\n",
