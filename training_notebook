        "# Define the range of n_estimators to test\n",
        "min_samples_leaf_range = [5, 10, 15, 20]\n",
        "\n",
        "cv_rmse_scores = []  # Store cross-validation RMSE scores\n",
        "prev_cv_rmse = None\n",
        "stabilization_threshold = 0.001  # Threshold for stopping training\n",
        "\n",
        "# Convert data to GPU arrays for faster processing\n",
        "X_train_cp = cp.array(X_train_mod_2.values)  # Features (Convert to CuPy)\n",
        "y_cm_cp = cp.array(y_train_mod_2['CM'].values)  # Target variable (Convert to CuPy)\n",
        "\n",
        "best_n_estimators = None  # Store the best number of trees\n",
        "\n",
        "# Use 5-Fold Cross-Validation (Using sklearn KFold for splitting)\n",
        "kf = KFold(n_splits=5, shuffle=True, random_state=42)\n",
        "\n",
        "for n in min_samples_leaf_range:\n",
        "    rf_gpu_cm = RandomForestRegressor(\n",
        "        n_estimators= 200 , max_features=max_ft, min_samples_leaf= n, random_state=42, n_streams=1\n",
        "    )\n",
        "\n",
        "    rmse_scores = []  # Store RMSE scores for each fold\n",
        "    for train_idx, test_idx in kf.split(X_train_mod):  # Sklearn KFold works with Pandas/NumPy\n",
        "        X_train_fold = X_train_cp[train_idx]\n",
        "        X_test_fold = X_train_cp[test_idx]\n",
        "        y_train_fold = y_cm_cp[train_idx]\n",
        "        y_test_fold = y_cm_cp[test_idx]\n",
        "\n",
        "        rf_gpu_cm.fit(X_train_fold, y_train_fold)\n",
        "        y_pred_fold = rf_gpu_cm.predict(X_test_fold)\n",
        "\n",
        "        # Compute RMSE\n",
        "        rmse_score = cp.sqrt(mean_squared_error(y_test_fold.get(), y_pred_fold.get()))\n",
        "        rmse_scores.append(rmse_score)\n",
        "\n",
        "\n",
        "\n",
        "    # Convert to a CuPy array\n",
        "    rmse_scores_cp = cp.array(rmse_scores)\n",
        "\n",
        "    # Explicitly convert CuPy array to NumPy array\n",
        "    rmse_scores_np = rmse_scores_cp.get()\n",
        "\n",
        "    mean_cv_rmse = np.mean(rmse_scores_np)  # Average CV RMSE score\n",
        "\n",
        "    mean_cv_rmse = mean_cv_rmse.tolist()  # transform it back to a list\n",
        "\n",
        "    cv_rmse_scores.append(mean_cv_rmse )\n",
        "\n",
        "\n",
        "    print(f\" min_samples_leaf : {n}, Cross-Validation RMSE: {mean_cv_rmse:.5f}\")\n",
